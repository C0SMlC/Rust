fn main() {
    let mut count = 0;
    loop {
        count += 1;
        println!("Count is: {}", count);
        if count == 5 {
            break;
        }
    }
    println!("Loop ended at count: {}", count);
}

fn main() {
    let mut number = 3;
    while number != 0 {
        println!("{}!", number);
        number -= 1;
    }
    println!("Liftoff!!!");
}

fn main() {
    for number in 1..4 {
        println!("{}!", number);
    }
    println!("Liftoff!!!");
}
